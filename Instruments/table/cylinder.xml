<?xml version="1.0"?>

<PropertyList>

 <path>cylinder.ac</path>

<!-- throttle -->

 <animation>
  <type>rotate</type>
  <object-name>ThrottleLever1</object-name>
  <property>controls/engines/engine[0]/throttle</property>
  <factor>-80</factor>
  <offset-deg>70</offset-deg>   
  <center>
   <x-m> 0.0</x-m>
   <y-m> 0.0</y-m>
   <z-m> 0.0</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>ThrottleLever1</object-name>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine[0]/throttle</property>
    <step>0.1</step>
    <min>0</min>
    <max>1</max>
    <wrap>0</wrap>
   </binding>
  </action>
  <action>
   <button>1</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine[0]/throttle</property>
    <step>-0.1</step>
    <min>0</min>
    <max>1</max>
    <wrap>0</wrap>
   </binding>
  </action>
 </animation>

