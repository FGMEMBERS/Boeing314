<!-- REGISTRATION -->

  <text>
   <name>Registration wing right</name>
   <type type="string">text-value</type>
   <property type="string">sim/model/immat</property>
   <format type="string">%s</format>
   <draw-text type="bool">true</draw-text>
   <font>Helvetica.txf</font>
   <character-size type="double">1.92</character-size>
   <character-aspect-ratio type="double">1.2</character-aspect-ratio>        <!-- compress -->
   <max-height>32</max-height>
   <max-width>80</max-width>
   <font-resolution>
    <width type="int">32</width>
    <height type="int">32</height>
   </font-resolution>
   <alignment>center-center</alignment>
   <offsets>
    <x-m>-2.260</x-m>
    <y-m>15.500</y-m>
    <z-m> 2.235</z-m>
    <heading-deg>90</heading-deg>         <!-- switch pitch to roll -->
    <pitch-deg>-2.2</pitch-deg>
    <roll-deg>2.5</roll-deg>
   </offsets>    
  </text>

  <text>
