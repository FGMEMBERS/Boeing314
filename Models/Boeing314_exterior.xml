    <y-m>-16.800</y-m>
    <z-m> 1.755</z-m>
    <heading-deg>90</heading-deg>         <!-- switch pitch to roll -->
    <pitch-deg>185.2</pitch-deg>
    <roll-deg>-1.9</roll-deg>
   </offsets>    
  </text>

