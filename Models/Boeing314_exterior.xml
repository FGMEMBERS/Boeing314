<?xml version="1.0"?>

<PropertyList>

  <model>
   <name>Anchor left</name>
   <path>Aircraft/Boeing314/Models/Exterior/anchor/anchor.xml</path>
   <offsets>
    <x-m>-15.568</x-m>
    <y-m>-0.277</y-m>
    <z-m> 0.818</z-m>
    <heading-deg>30</heading-deg>
    <pitch-deg>-15</pitch-deg>
