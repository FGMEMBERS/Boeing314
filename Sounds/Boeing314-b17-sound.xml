   <name>engine low1</name>
   <mode>looped</mode>
   <path>Aircraft/Boeing314/Sounds/17_1.wav</path>
   <condition>
    <or>
     <and>
      <property>/sim/current-view/internal</property>
      <property>/engines/engine[0]/running</property>
      <greater-than-equals>
       <property>/engines/engine[0]/mp-osi</property>
       <value>6.2</value>
