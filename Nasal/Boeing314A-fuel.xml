<?xml version="1.0"?>

<!-- Fuel configurations : in JSBSim, 1 gal US = 6.6 lb.

     Default is max landing weight.
-->

<PropertyList>

<!-- Max landing weight (80000 lb / 5041 gallons fuel), -->

    <filling n="0">
     <comment>max landing</comment>
     <tank n="0"></tank>
     <tank n="1"></tank>
     <tank n="2"></tank>
     <tank n="3"></tank>
     <tank n="4">
      <level-gal_us>130.0</level-gal_us>
     </tank>
     <tank n="5"></tank>
     <tank n="6"></tank>
    </filling>

<!-- Max takeoff weight (84000 lb / 5171 gallons fuel). -->

    <filling n="1">
     <comment>max takeoff</comment>
    </filling>

<!-- Empty tanks (center is emptied first) : for FDM and brakes test. -->

    <filling n="2">
     <comment>empty</comment>
     <tank n="0">
      <level-gal_us>75.0</level-gal_us>
     </tank>
     <tank n="1">
      <level-gal_us>75.0</level-gal_us>
     </tank>
     <tank n="2">
      <level-gal_us>75.0</level-gal_us>
     </tank>
     <tank n="3">
      <level-gal_us>75.0</level-gal_us>
     </tank>
     <tank n="4">
      <level-gal_us>0.0</level-gal_us>
     </tank>
     <tank n="5">
      <level-gal_us>0.0</level-gal_us>
     </tank>
     <tank n="6">
      <level-gal_us>0.0</level-gal_us>
     </tank>
    </filling>

<!-- other -->

</PropertyList>


